{
  "name": "h4rd1ideveloper/fullstack-boilerplate",
  "license": "MIT",
  "authors": [
    {
      "name": "Yan Santos Policarpo",
      "email": "policarpo@ice.ufjf.br"
    }
  ],
  "description": "boilerplate",
  "scripts": {
    "start-dev": "php -S localhost:8080 -t ./",
    "test": "phpunit --no-configuration src/__TEST__ --teamcity"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Server\\": "core/routes",
      "Psr\\Http\\Message\\": "core/http/",
      "Lib\\": "core/lib/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7",
    "http-interop/http-factory-tests": "dev-master"
  },
  "require": {
    "php": ">=7.1",
    "psr/http-message": "~1.0",
    "psr/http-factory": "^1.0",
    "ralouphie/getallheaders": "^2.0.5 || ^3.0.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "monolog/monolog": "^2.0",
    "firebase/php-jwt": "^5.0",
    "robmorgan/phinx": "^0.11.1",
    "twig/twig": "^3.0",
    "fzaninotto/faker": "^1.9",
    "illuminate/database":"*"
  },
  "provide": {
    "psr/http-factory-implementation": "1.0",
    "psr/http-message-implementation": "1.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
